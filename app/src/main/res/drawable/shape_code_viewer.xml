<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="-10dp" android:bottom="-10dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke
                android:width="4dp"
                android:color="@color/colorPrimaryDark"
                android:dashWidth="182dp"
                android:dashGap="315dp" />
            <corners android:radius="4dp" />

            <padding
                android:bottom="8dp"
                android:left="8dp"
                android:right="8dp"
                android:top="8dp" />

            <solid android:color="@color/white" />

        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke
                android:width="4dp"
                android:color="@color/colorPrimaryDark"
                android:dashWidth="90dp"
                android:dashGap="901dp" />
            <corners android:radius="4dp" />

            <padding
                android:bottom="8dp"
                android:left="8dp"
                android:right="8dp"
                android:top="8dp" />

            <solid android:color="@color/white" />

        </shape>
    </item>
</layer-list>